{
    "version": 8,
    "configurePresets": [
        {
            "name": "Clang 21.1.8 x86_64-w64-windows-gnu (mingw64)",
            "displayName": "Clang 21.1.8 x86_64-w64-windows-gnu (mingw64)",
            "description": "Using compilers: C = c:\\msys64\\mingw64\\bin\\clang.exe, CXX = c:\\msys64\\mingw64\\bin\\clang++.exe",
            "generator": "MinGW Makefiles",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "c:/msys64/mingw64/bin/clang.exe",
                "CMAKE_CXX_COMPILER": "c:/msys64/mingw64/bin/clang++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "Custom configure preset",
            "displayName": "Custom configure preset",
            "description": "Sets Ninja generator, build and install directory",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}"
            }
        }
    ]
}